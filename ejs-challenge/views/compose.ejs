<%- include('partials/header') %>
<h1>Compose</h1>
<form action="/compose" method="post">
<div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" placeholder="Title" value="" >
</div>



<div class="form-group">
    <label for="postBody">Body</label>
<textarea name="postBody" class="form-control" rows="5" cols="100" placeholder=""></textarea>
</div>
<button type="submit" class="btn btn-primary" name="button" >Publish</button>
</form>
<%- include('partials/footer') %>